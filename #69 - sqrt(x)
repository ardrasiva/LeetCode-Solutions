class Solution(object):
    def mySqrt(self, x):
        """
        :type x: int
        :rtype: int
        """
        left = 0
        right = x
        res = 0
        while left <=right:
            mid = (left+right) // 2
            sqr = mid*mid
            if sqr == x:
                return mid
            elif sqr < x:
                res = mid
                left = mid+1
            else:
                right = mid-1
        return res
